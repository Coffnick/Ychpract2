<Window x:Class="Ychpract2.Choice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ychpract2"
        mc:Ignorable="d"
        Title="Choice" Height="450" Width="800">
    <Grid>
        <Button Content="Товары" HorizontalAlignment="Left" Margin="160,239,0,0" VerticalAlignment="Top" Height="135" Width="206" FontSize="24" Background="#FF31E6BD" Click="Goods_click"/>
        <Button Content="Клиенты" HorizontalAlignment="Left" Margin="440,239,0,0" VerticalAlignment="Top" Height="135" Width="206" FontSize="24" Background="#FF31E6BD" Click="Client_click"/>
        <Button Content="Поставщики" HorizontalAlignment="Left" Margin="160,60,0,0" VerticalAlignment="Top" Height="135" Width="206" FontSize="24" Background="#FF31E6BD" Click="Sup_click"/>
        <Button Content="Накладные" HorizontalAlignment="Left" Margin="440,60,0,0" VerticalAlignment="Top" Height="135" Width="206" FontSize="24" Background="#FF31E6BD" Click="Naklad_click"/>
        <Button Content="ВЫХОД" Background="#FF31E6BD" Click="Exit" Margin="10,10,0,0" HorizontalAlignment="Left" Width="90" Height="28" VerticalAlignment="Top"/>
    </Grid>
</Window>
